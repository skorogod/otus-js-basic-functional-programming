{
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/jest": "^29.5.6",
    "@webpack-cli/generators": "^3.0.7",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm": "^10.2.1",
    "prettier": "^3.0.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "version": "1.0.0",
  "description": "My webpack project",
  "name": "my-webpack-project",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "test": "jest --coverage --env=jsdom"
  },
  "lint-staged": {
    "*ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.scss": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "typescript-eslint": "*"
  }
}
